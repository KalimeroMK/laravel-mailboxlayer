{
  "name": "ashallendesign/laravel-mailboxlayer",
  "description": "A Laravel package for validating emails using the Mailbox Layer API.",
  "type": "library",
  "homepage": "https://github.com/ash-jc-allen/laravel-mailboxlayer",
  "license": "MIT",
  "authors": [
    {
      "name": "Ash Allen",
      "email": "mail@ashallendesign.co.uk"
    }
  ],
  "keywords": [
    "ashallendesign",
    "laravel",
    "laravel-package",
    "mailboxlayer",
    "email",
    "validation"
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/container": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "AshAllenDesign\\MailboxLayer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AshAllenDesign\\MailboxLayer\\Providers\\MailboxLayerProvider"
      ]
    }
  }
}
